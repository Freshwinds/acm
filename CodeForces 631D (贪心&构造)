# c++
#include<iostream>
using namespace std;
const int maxn=110;
int rt[maxn],ct[maxn];//行和列反转的次数；
int rs[maxn],cs[maxn];//行和列的总和
int a[maxn][maxn];
int n,m;
int getsr()
{
    for(int i=1;i<=n;i++)
    {
        if(rs[i]<0)
        {

            return i;
        }
    }
    return 0;
}
int getsc( )
{
    for(int i=1;i<=m;i++)
    {
        if(cs[i]<0)
        {

            return i;
        }
    }
    return 0;
}
int main()
{
    cin>>n>>m;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=m;j++)
            {
                cin>>a[i][j];
                rs[i]+=a[i][j];
                cs[j]+=a[i][j];
            }
    }
    int x=0,y=0;
    while((x=getsr())||(y=getsc()))
    {

        if(x)
        {
            rs[x]=-rs[x];
            rt[x]++;
            for(int i=1;i<=m;i++)
            {
                cs[i]-=(2*a[x][i]);
                a[x][i]=-a[x][i];
            }
        }
        else
        {
            ct[y]++;
            cs[y]=-cs[y];
            for(int i=1;i<=n;i++)
            {
                rs[i]-=(2*a[i][y]);
                a[i][y]=-a[i][y];
            }
        }
    }
    int t1=0,t2=0;
    for(int i=1;i<=n;i++)
    {
        if(rt[i]%2) t1++;
    }
    cout<<t1<<" ";
    for(int i=1;i<=n;i++)
    {
        if(rt[i]%2) cout<<i<<" ";
    }
    cout<<endl;
    for(int i=1;i<=m;i++)
    {
        if(ct[i]%2) t2++;
    }
    cout<<t2<<" ";
    for(int i=1;i<=m;i++)
    {
        if(ct[i]%2) cout<<i<<" ";
    }
    cout<<endl;
    return 0;
}
